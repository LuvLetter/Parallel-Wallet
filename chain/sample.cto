/**
 * Sample business network definition.
 */
namespace org.acme.sample

asset partner_info identified by info_Id {
  o String info_Id
  --> partner partnerId
  o Long sum_exchange
  o Long sum_asset
}

participant partner identified by partnerId {
  o String partnerId
  o String indicator
  o Boolean vaild_user
}

transaction trans {
  --> partner_info asset
  o Long exchange
  o Long For_asset
}

event SampleEvent {
  --> partner_info asset
}
